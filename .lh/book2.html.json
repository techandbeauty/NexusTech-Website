            "name": "Commit-0",
            "content": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n  <meta charset=\"UTF-8\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <meta name=\"description\" content=\"Book a free consultation with Nexus Tech Digital Solutions to discuss your project.\">\n  <title>Book a Consultation | Nexus Tech Digital Solutions</title>\n  <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n  <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n  <link href=\"https://fonts.googleapis.com/css2?family=Outfit:wght@400;500;600;700;800&family=Source+Sans+3:wght@400;500;600&display=swap\" rel=\"stylesheet\">\n  <link rel=\"stylesheet\" href=\"css/styles.css\">\n  <style>\n    .calendar-embed {\n      background: var(--color-white);\n      border: 1px solid var(--color-gray-100);\n      border-radius: var(--radius-lg);\n      overflow: hidden;\n      min-height: 650px;\n    }\n    .calendar-placeholder {\n      padding: 3rem;\n      text-align: center;\n      min-height: 650px;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: center;\n      background: var(--color-off-white);\n    }\n    .calendar-placeholder svg {\n      color: var(--color-accent);\n      margin-bottom: 1.5rem;\n    }\n    .calendar-placeholder h3 {\n      margin-bottom: 0.5rem;\n    }\n    .calendar-placeholder p {\n      max-width: 400px;\n      margin-bottom: 1.5rem;\n    }\n    .setup-box {\n      background: var(--color-white);\n      border: 1px solid var(--color-gray-200);\n      border-radius: var(--radius-md);\n      padding: 1.5rem;\n      margin-top: 2rem;\n      text-align: left;\n      max-width: 500px;\n    }\n    .setup-box h4 {\n      font-size: 0.9375rem;\n      margin-bottom: 0.75rem;\n    }\n    .setup-box ol {\n      margin-left: 1.25rem;\n      font-size: 0.875rem;\n      color: var(--color-gray-600);\n    }\n    .setup-box ol li {\n      margin-bottom: 0.5rem;\n      list-style: decimal;\n    }\n    .setup-box code {\n      background: var(--color-off-white);\n      padding: 0.125rem 0.375rem;\n      border-radius: 4px;\n      font-size: 0.8125rem;\n    }\n    .booking-step {\n      display: flex;\n      gap: 1rem;\n      margin-bottom: 1.25rem;\n    }\n    .step-number {\n      width: 36px;\n      height: 36px;\n      background: var(--color-accent);\n      color: white;\n      border-radius: 50%;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      font-family: var(--font-display);\n      font-weight: 700;\n      font-size: 0.875rem;\n      flex-shrink: 0;\n    }\n    .step-content h4 {\n      margin-bottom: 0.125rem;\n      font-size: 1rem;\n    }\n    .step-content p {\n      margin-bottom: 0;\n      font-size: 0.9375rem;\n    }\n    .form-success {\n      display: none;\n      background: #d4edda;\n      border: 1px solid #c3e6cb;\n      color: #155724;\n      padding: 1rem;\n      border-radius: 8px;\n      margin-bottom: 1rem;\n    }\n    .form-error {\n      display: none;\n      background: #f8d7da;\n      border: 1px solid #f5c6cb;\n      color: #721c24;\n      padding: 1rem;\n      border-radius: 8px;\n      margin-bottom: 1rem;\n    }\n  </style>\n</head>\n<body>\n  <!-- Header -->\n  <header class=\"header\">\n    <div class=\"container\">\n      <div class=\"header-inner\">\n        <a href=\"index.html\" class=\"logo\">\n          <div class=\"logo-icon\">N</div>\n          <span>Nexus Tech</span>\n        </a>\n        <nav class=\"nav\">\n          <ul class=\"nav-links\">\n            <li><a href=\"index.html\" class=\"nav-link\">Home</a></li>\n            <li><a href=\"about.html\" class=\"nav-link\">About</a></li>\n            <li><a href=\"services.html\" class=\"nav-link\">Services</a></li>\n            <li><a href=\"portfolio.html\" class=\"nav-link\">Portfolio</a></li>\n            <li><a href=\"faq.html\" class=\"nav-link\">FAQ</a></li>\n            <li><a href=\"contact.html\" class=\"nav-link\">Contact</a></li>\n          </ul>\n          <a href=\"book.html\" class=\"btn btn-primary\">Book Consultation</a>\n        </nav>\n        <button class=\"mobile-menu-btn\" aria-label=\"Toggle menu\">\n          <span></span><span></span><span></span>\n        </button>\n      </div>\n    </div>\n  </header>\n\n  <div class=\"mobile-menu\">\n    <a href=\"index.html\" class=\"mobile-nav-link\">Home</a>\n    <a href=\"about.html\" class=\"mobile-nav-link\">About</a>\n    <a href=\"services.html\" class=\"mobile-nav-link\">Services</a>\n    <a href=\"portfolio.html\" class=\"mobile-nav-link\">Portfolio</a>\n    <a href=\"faq.html\" class=\"mobile-nav-link\">FAQ</a>\n    <a href=\"contact.html\" class=\"mobile-nav-link\">Contact</a>\n  </div>\n\n  <!-- Page Header -->\n  <section class=\"page-header\">\n    <div class=\"container\">\n      <h1>Book a Consultation</h1>\n      <p class=\"lead\">Schedule a free 30-minute discovery call to discuss your project.</p>\n    </div>\n  </section>\n\n  <!-- Booking Section -->\n  <section class=\"section\">\n    <div class=\"container\">\n      <div class=\"grid grid-2\" style=\"gap: 4rem; align-items: start;\">\n        <!-- Left: Info -->\n        <div>\n          <h2 style=\"margin-bottom: 1rem;\">Let's Talk About Your Project</h2>\n          <p>During our discovery call, we'll discuss your business goals, technical requirements, and timeline. No sales pressure‚Äîjust an honest conversation about how we can help.</p>\n\n          <div style=\"margin: 2rem 0;\">\n            <div class=\"booking-step\">\n              <div class=\"step-number\">1</div>\n              <div class=\"step-content\">\n                <h4>Pick a Time</h4>\n                <p>Choose a date and time that works for your schedule.</p>\n              </div>\n            </div>\n            <div class=\"booking-step\">\n              <div class=\"step-number\">2</div>\n              <div class=\"step-content\">\n                <h4>Discovery Call</h4>\n                <p>We'll discuss your project, answer questions, and explore solutions.</p>\n              </div>\n            </div>\n            <div class=\"booking-step\">\n              <div class=\"step-number\">3</div>\n              <div class=\"step-content\">\n                <h4>Receive Your Proposal</h4>\n                <p>Get a detailed proposal with scope, timeline, and fixed pricing within 48 hours.</p>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"contact-card\">\n            <h4>\n              <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z\"></path><polyline points=\"22,6 12,13 2,6\"></polyline></svg>\n              Prefer Email?\n            </h4>\n            <p><a href=\"mailto:nexustech813@gmail.com\">send us a message</a></p>\n          </div>\n\n          <div class=\"contact-card\">\n            <h4>\n              <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z\"></path></svg>\n              Call Us\n            </h4>\n            <p><a href=\"tel:+12027094556\">(202) 709-4556</a></p>\n          </div>\n\n          <div class=\"info-box\" style=\"margin-top: 1.5rem;\">\n            <h4>üèõÔ∏è Government Contracting?</h4>\n            <p>For RFPs, teaming agreements, and federal inquiries, please use our dedicated form.</p>\n            <a href=\"government.html\" class=\"btn btn-white btn-sm\" style=\"margin-top: 1rem;\">Government Inquiries ‚Üí</a>\n          </div>\n        </div>\n\n        <!-- Right: Calendar or Form -->\n        <div>\n          <div class=\"calendar-embed\">\n            <div class=\"calendar-placeholder\">\n              <svg width=\"64\" height=\"64\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"1.5\">\n                <rect x=\"3\" y=\"4\" width=\"18\" height=\"18\" rx=\"2\" ry=\"2\"></rect>\n                <line x1=\"16\" y1=\"2\" x2=\"16\" y2=\"6\"></line>\n                <line x1=\"8\" y1=\"2\" x2=\"8\" y2=\"6\"></line>\n                <line x1=\"3\" y1=\"10\" x2=\"21\" y2=\"10\"></line>\n              </svg>\n              <h3>Schedule Your Call</h3>\n              <p>Fill out the form below and we'll get back to you within 24 hours to schedule your consultation.</p>\n              \n              <!-- Formspree Form -->\n              <div class=\"form-container\" style=\"width: 100%; max-width: 400px; margin-top: 1rem; text-align: left;\">\n                \n                <!-- Success Message -->\n                <div class=\"form-success\" id=\"formSuccess\">\n                  <strong>‚úì Request Received!</strong><br>\n                  We'll contact you within 24 hours to schedule your consultation.\n                </div>\n\n                <!-- Error Message -->\n                <div class=\"form-error\" id=\"formError\">\n                  <strong>‚ö† Oops!</strong><br>\n                  Something went wrong. Please try again or email us directly.\n                </div>\n\n                <form action=\"https://formspree.io/f/xeekpgwo\" method=\"POST\" id=\"consultationForm\">\n                  \n                  <div class=\"form-group\">\n                    <label class=\"form-label\">Your Name <span class=\"required\">*</span></label>\n                    <input type=\"text\" name=\"name\" class=\"form-input\" placeholder=\"Full name\" required>\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label class=\"form-label\">Email <span class=\"required\">*</span></label>\n                    <input type=\"email\" name=\"email\" class=\"form-input\" placeholder=\"you@example.com\" required>\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label class=\"form-label\">Phone <span class=\"required\">*</span></label>\n                    <input type=\"tel\" name=\"phone\" class=\"form-input\" placeholder=\"(555) 123-4567\" required>\n                  </div>\n\n                  <div class=\"form-group\">\n                    <label class=\"form-label\">What do you need help with?</label>\n                    <select name=\"service_interest\" class=\"form-select\">\n                      <option value=\"\">Select...</option>\n                      <option value=\"Website Development\">Website Development</option>\n                      <option value=\"Mobile App\">Mobile App</option>\n                      <option value=\"Business Automation\">Business Automation</option>\n                      <option value=\"AI Integration\">AI Integration</option>\n                      <option value=\"Branding & Design\">Branding & Design</option>\n                      <option value=\"IT Support\">IT Support</option>\n                      <option value=\"Other / Not Sure\">Other / Not Sure</option>\n                    </select>\n                  </div>\n\n                  <div class=\"form-group\">\n                    <label class=\"form-label\">Brief project description</label>\n                    <textarea name=\"message\" class=\"form-textarea\" placeholder=\"Tell us a bit about your project...\" style=\"min-height: 80px;\"></textarea>\n                  </div>\n\n                  <!-- Hidden field for subject line -->\n                  <input type=\"hidden\" name=\"_subject\" value=\"New Consultation Request - Nexus Tech\">\n\n                  <button type=\"submit\" class=\"btn btn-primary btn-block\" id=\"submitBtn\">\n                    Request Consultation\n                  </button>\n                </form>\n              </div>\n\n              <div class=\"setup-box\">\n                <h4>üöÄ Want Instant Booking?</h4>\n                <ol>\n                  <li>Create a free account at <a href=\"https://cal.com\" target=\"_blank\">cal.com</a></li>\n                  <li>Connect your Google Calendar</li>\n                  <li>Create a \"30 Min Discovery Call\" event</li>\n                  <li>Replace this form with your embed code</li>\n                </ol>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n\n  <!-- Footer -->\n  <footer class=\"footer\">\n    <div class=\"container\">\n      <div class=\"footer-grid\">\n        <div class=\"footer-brand\">\n          <a href=\"index.html\" class=\"logo\">\n            <div class=\"logo-icon\">N</div>\n            <span>Nexus Tech</span>\n          </a>\n          <p>Custom websites, mobile apps, and automation solutions‚Äîbuilt by engineers who understand what growing businesses actually need.</p>\n          <div class=\"footer-social\">\n            <a href=\"https://www.instagram.com/nexustechdigitalsolutions\" target=\"_blank\" aria-label=\"Instagram\">\n              <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><rect x=\"2\" y=\"2\" width=\"20\" height=\"20\" rx=\"5\" ry=\"5\"></rect><path d=\"M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z\"></path><line x1=\"17.5\" y1=\"6.5\" x2=\"17.51\" y2=\"6.5\"></line></svg>\n            </a>\n            <a href=\"https://www.facebook.com/share/1De6QUqb3s/?mibextid=wwXIfr\" target=\"_blank\" aria-label=\"Facebook\">\n              <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\"><path d=\"M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z\"></path></svg>\n            </a>\n          </div>\n        </div>\n        <div>\n          <h4 class=\"footer-heading\">Services</h4>\n          <ul class=\"footer-links\">\n            <li><a href=\"services.html#websites\">Website Development</a></li>\n            <li><a href=\"services.html#mobile\">Mobile Apps</a></li>\n            <li><a href=\"services.html#automation\">Business Automation</a></li>\n            <li><a href=\"services.html#branding\">Branding & Design</a></li>\n            <li><a href=\"services.html#support\">IT Support</a></li>\n          </ul>\n        </div>\n        <div>\n          <h4 class=\"footer-heading\">Company</h4>\n          <ul class=\"footer-links\">\n            <li><a href=\"about.html\">About Us</a></li>\n            <li><a href=\"portfolio.html\">Portfolio</a></li>\n            <li><a href=\"faq.html\">FAQ</a></li>\n            <li><a href=\"government.html\">Government</a></li>\n            <li><a href=\"contact.html\">Contact</a></li>\n          </ul>\n        </div>\n        <div>\n          <h4 class=\"footer-heading\">Contact</h4>\n          <ul class=\"footer-links\">\n            <li><a href=\"mailto:info@nexustechdigitalsolutions.net\">info@nexustechdigitalsolutions.net</a></li>\n            <li><a href=\"tel:+12027094556\">(202) 709-4556</a></li>\n            <li>Norfolk, Virginia</li>\n          </ul>\n        </div>\n      </div>\n      <div class=\"footer-bottom\">\n        <p>&copy; 2024 Nexus Tech Digital Solutions. All rights reserved.</p>\n      </div>\n    </div>\n  </footer>\n\n  <script>\n    // Mobile menu\n    const mobileMenuBtn = document.querySelector('.mobile-menu-btn');\n    const mobileMenu = document.querySelector('.mobile-menu');\n    mobileMenuBtn?.addEventListener('click', () => mobileMenu.classList.toggle('active'));\n\n    // Form handling\n    const form = document.getElementById('consultationForm');\n    const submitBtn = document.getElementById('submitBtn');\n    const successMsg = document.getElementById('formSuccess');\n    const errorMsg = document.getElementById('formError');\n\n    form.addEventListener('submit', async (e) => {\n      e.preventDefault();\n      \n      // Disable button and show loading state\n      submitBtn.disabled = true;\n      submitBtn.textContent = 'Sending...';\n      \n      // Hide any previous messages\n      successMsg.style.display = 'none';\n      errorMsg.style.display = 'none';\n\n      try {\n        const response = await fetch(form.action, {\n          method: 'POST',\n          body: new FormData(form),\n          headers: {\n            'Accept': 'application/json'\n          }\n        });\n\n        if (response.ok) {\n          // Success!\n          successMsg.style.display = 'block';\n          form.reset();\n          \n          // Re-enable button\n          submitBtn.disabled = false;\n          submitBtn.textContent = 'Request Consultation';\n        } else {\n          // Error from server\n          throw new Error('Server error');\n        }\n      } catch (error) {\n        // Network or other error\n        errorMsg.style.display = 'block';\n        submitBtn.disabled = false;\n        submitBtn.textContent = 'Request Consultation';\n      }\n    });\n  </script>\n</body>\n</html>"
        }
